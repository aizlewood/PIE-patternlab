<div class="page" id="page">
	{{> organisms-header-homepage-logged-in }}
	<div role="main">
		{{# emergency }}
			{{> molecules-alert:error }}
		{{/ emergency }}
		{{# hero }}
			{{> molecules-block-hero }}
		{{/ hero}}

		<div class="g g-3up">
			{{# touts}}
				<div class="gi">
					{{> molecules-block-inset }}
				</div>
			{{/ touts}}
		</div><!--end 3up-->

		<hr />

		<div class="inner">
			<div class="row">
				<div class="half"><br />{{> molecules-topshelf-title }}</div>
				<div class="half right"></div>
			</div>		
			{{> organisms-job-listing }}
		</div>

	</div><!--End role=main-->
	{{> organisms-prefooter }}
	{{> organisms-footer }}
</div>