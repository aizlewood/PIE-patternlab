		<h1 class="section-title">Our Outdoor Blog</h1>
		<div class="l-two-col">
			<div class="l-main">
				{{> organisms-latest-posts }}
				{{> molecules-pagination }}
			</div><!--end l-main-->

			<div class="l-sidebar">
				{{> organisms-recent-links }}
			</div><!--end l-sidebar-->
